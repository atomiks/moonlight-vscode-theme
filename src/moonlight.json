    "settings.numberInputForeground": "{gray10}",
    "settings.numberInputBackground": "{gray2}",
    "settings.numberInputBorder": "#00000066",
    "settings.textInputForeground": "{gray10}",
    "settings.textInputBackground": "{gray2}",
    "settings.textInputBorder": "#00000066",
    "settings.headerForeground": "{blue}",
    "settings.modifiedItemIndicator": "{blue}",
    "settings.checkboxBackground": "{gray1}",
    "settings.checkboxForeground": "{gray10}",
    "settings.checkboxBorder": "#00000066"
