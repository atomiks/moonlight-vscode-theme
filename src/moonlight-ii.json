    "terminal.ansiWhite": "{gray10}",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBlue": "{blue}",
    "terminal.ansiCyan": "{cyan}",
    "terminal.ansiGreen": "{green}",
    "terminal.ansiMagenta": "{pink}",
    "terminal.ansiRed": "{red}",
    "terminal.ansiYellow": "{yellow}",
    "terminal.ansiBrightWhite": "{gray10}",
    "terminal.ansiBrightBlack": "{gray7}",
    "terminal.ansiBrightBlue": "{blue}",
    "terminal.ansiBrightCyan": "{cyan}",
    "terminal.ansiBrightGreen": "{green}",
    "terminal.ansiBrightMagenta": "{pink}",
    "terminal.ansiBrightRed": "{red}",
    "terminal.ansiBrightYellow": "{yellow}",
    "terminal.border": "{gray5}",
    "scrollbarSlider.background": "{gray7}30",
    "scrollbarSlider.hoverBackground": "{gray8}30",
    "scrollbarSlider.activeBackground": "{blue}",
    "minimap.findMatchHighlight": "{cyan}cc",
    "minimap.selectionHighlight": "{cyan}33",
    "minimapGutter.addedBackground": "{green}66",
    "minimapGutter.modifiedBackground": "{blue}66",
    "editorSuggestWidget.background": "{gray2}",
    "editorSuggestWidget.foreground": "{gray8}",
    "editorSuggestWidget.highlightForeground": "{cyan}",
    "editorSuggestWidget.selectedBackground": "{gray5}",
    "editorSuggestWidget.border": "#00000033",
    "editorError.foreground": "{darkRed}",
    "editorWarning.foreground": "{yellow}cc",
    "editorWidget.background": "{gray3}",
    "editorWidget.resizeBorder": "{blue}",
    "editorMarkerNavigation.background": "{gray10}05",
    "widget.shadow": "#00000033",
    "panel.border": "#00000033",
    "panel.background": "{gray3}",
    "panel.dropBackground": "{gray10}",
    "panelTitle.inactiveForeground": "{gray7}",
    "panelTitle.activeForeground": "{gray10}",
    "panelTitle.activeBorder": "{blue}",
    "terminalCursor.foreground": "{blue}",
    "diffEditor.insertedTextBackground": "{green}15",
    "diffEditor.removedTextBackground": "{darkRed}20",
    "notifications.background": "{gray2}",
    "notifications.foreground": "{gray10}",
    "notificationLink.foreground": "{blue}",
    "badge.background": "{darkBlue}",
    "badge.foreground": "#ffffffee",
    "button.background": "{darkBlue}",
    "button.hoverBackground": "{darkestBlue}",
    "button.foreground": "#ffffff",
    "extensionButton.prominentBackground": "{darkBlue}",
    "extensionButton.prominentHoverBackground": "{sky}cc",
    "peekView.border": "#00000030",
    "peekViewEditor.background": "{gray10}05",
    "peekViewTitle.background": "{gray10}05",
    "peekViewResult.background": "{gray10}05",
    "peekViewEditorGutter.background": "{gray10}05",
    "peekViewTitleDescription.foreground": "{gray10}60",
    "peekViewResult.matchHighlightBackground": "{gray7}50",
    "peekViewEditor.matchHighlightBackground": "{gray7}50",
    "debugToolBar.background": "{gray3}",
    "pickerGroup.foreground": "{blue}",
    "gitDecoration.deletedResourceForeground": "{darkRed}dd",
    "gitDecoration.conflictingResourceForeground": "{yellow}cc",
    "gitDecoration.modifiedResourceForeground": "{blue}ee",
    "gitDecoration.untrackedResourceForeground": "{lightTeal}cc",
    "gitDecoration.ignoredResourceForeground": "#777fabaa",
    "gitlens.trailingLineForegroundColor": "{gray7}aa",
    "editorCodeLens.foreground": "{gray7}",
    "peekViewResult.selectionBackground": "{gray7}70",
    "breadcrumb.background": "{gray4}",
    "breadcrumb.foreground": "{gray7}",
    "breadcrumb.focusForeground": "{gray10}",
    "breadcrumb.activeSelectionForeground": "{blue}",
    "breadcrumbPicker.background": "{gray3}",
    "menu.background": "{gray3}",
    "menu.foreground": "{gray10}",
    "menu.selectionBackground": "#00000050",
    "menu.selectionForeground": "{blue}",
    "menu.selectionBorder": "#00000030",
    "menu.separatorBackground": "{gray10}",
    "menubar.selectionBackground": "#00000030",
    "menubar.selectionForeground": "{blue}",
    "menubar.selectionBorder": "#00000030",
    "settings.dropdownForeground": "{gray10}",
    "settings.dropdownBackground": "{gray5}",
    "settings.dropdownBorder": "{gray2}",
    "settings.numberInputForeground": "{gray10}",
    "settings.numberInputBackground": "{gray2}",
    "settings.numberInputBorder": "#00000066",
    "settings.textInputForeground": "{gray10}",
    "settings.textInputBackground": "{gray2}",
    "settings.textInputBorder": "#00000066",
    "settings.headerForeground": "{blue}",
    "settings.modifiedItemIndicator": "{blue}",
    "settings.checkboxBackground": "{gray1}",
    "settings.checkboxForeground": "{gray10}",
    "settings.checkboxBorder": "#00000066"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "{saturatedGray}",
        "fontStyle": "{fontStyle}"
      }
    },
    {
      "name": "Variables and Plain Text",
      "scope": [
        "variable",
        "support.variable",
        "string constant.other.placeholder",
        "text.html"
      ],
      "settings": {
        "foreground": "{gray10}"
      }
    },

    {
      "name": "DOM Variables",
      "scope": [
        "support.variable.dom",
        "support.constant.math",
        "support.type.object.module",
        "support.variable.object.process",
        "support.constant.json"
      ],
      "settings": {
        "foreground": "{yellow}"
      }
    },
    {
      "name": "Nil",
      "scope": ["constant.language.undefined", "constant.language.null"],
      "settings": {
        "foreground": "{desaturatedGray}"
      }
    },
    {
      "name": "PHP Constants",
      "scope": ["constant.other.php"],
      "settings": {
        "foreground": "{yellow}"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "{darkRed}"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "{purple}"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.other.important"
      ],
      "settings": {
        "foreground": "{purple}"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword.control", "storage"],
      "settings": {
        "fontStyle": "{fontStyle}"
      }
    },
    {
      "name": "Interpolation",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "{cyan}"
      }
    },
    {
      "name": "Spread",
      "scope": ["keyword.operator.spread", "keyword.operator.rest"],
      "settings": {
        "foreground": "{red}",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.operator",
        "keyword.control",
        "punctuation",
        "punctuation.definition.string",
        "punctuation.support.type.property-name",
        "text.html.vue-html meta.tag",
        "punctuation.definition.keyword",
        "punctuation.terminator.rule",
        "punctuation.definition.entity",
        "constant.other.color",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.block.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "meta.property-list",
        "meta.brace.square",
        "keyword.other.template",
        "keyword.other.substitution",
        "string.other.begin.code",
        "string.other.end.code"
      ],
      "settings": {
        "foreground": "{cyan}"
      }
    },
    {
      "name": "Dark Punctuation",
      "scope": ["punctuation.definition.template-expression"],
      "settings": {
        "foreground": "{red}"
      }
    },
    {
      "name": "Keyword Control",
      "scope": ["keyword.control"],
      "settings": {
        "fontStyle": "{fontStyle}"
      }
    },

    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "{red}"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "variable.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "{blue}"
      }
    },
    {
      "name": "Support Function",
      "scope": ["support.function", "meta.function-call entity.name.function"],
      "settings": {
        "foreground": "{sky}"
      }
    },
    {
      "name": "C-related Block Level Variables",
      "scope": ["source.cpp meta.block variable.other"],
      "settings": {
        "foreground": "{red}"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "{red}"
      }
    },
    {
      "name": "Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "variable.other.constant",
        "constant.language",
        "keyword.other.type.php",
        "storage.type.php",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "{lightRed}"
      }
    },
    {
      "name": "Number, Boolean",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.json",
        "constant.language.infinity",
        "constant.language.nan"
      ],
      "settings": {
        "foreground": "{orange}"
      }
    },
    {
      "name": "Function Argument",
      "scope": [
        "variable.parameter.function.language.special",
        "variable.parameter",
        "meta.function.parameter variable"
      ],
      "settings": {
        "foreground": "{pink}"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "meta.attribute-selector",
        "markup.inline.raw.code"
      ],
      "settings": {
        "foreground": "{green}"
      }
    },
    {
      "name": "Object",
      "scope": ["variable.other.object"],
      "settings": {
        "foreground": "{yellow}"
      }
    },
    {
      "name": "Object Key",
      "scope": [
        "meta.object-literal.key",
        "string.alias.graphql",
        "string.unquoted.graphql",
        "string.unquoted.alias.graphql",
        "meta.field.declaration.ts variable.object.property",
        "variable.object.property"
      ],
      "settings": {
        "foreground": "{teal}"
      }
    },
    {
      "name": "Nested Object Property",
      "scope": ["meta.object.member", "variable.other.object.property"],
      "settings": {
        "foreground": "{gray8}"
      }
    },
    {
      "name": "Object Property",
      "scope": [
        "variable.other.property",
        "support.variable.property",
        "support.variable.property.dom"
      ],
      "settings": {
        "foreground": "{gray8}"
      }
    },
    {
      "name": "Haskell Constants",
      "scope": ["source.haskell constant.other.haskell"],
      "settings": {
        "foreground": "{lightRed}"
      }
    },
    {
      "name": "Haskell Imports",
      "scope": ["source.haskell meta.import.haskell entity.name.namespace"],
      "settings": {
        "foreground": "{gray10}"
      }
    },
    {
      "name": "Types Fixes",
      "scope": [
        "source.haskell storage.type",
        "source.c storage.type",
        "source.java storage.type"
      ],
      "settings": {
        "foreground": "{yellow}"
      }
    },
    {
      "name": "Lambda Arrow",
      "scope": ["storage.type.function"],
      "settings": {
        "foreground": "{purple}"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "{yellow}"
      }
    },
    {
      "name": "Entity types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "{orange}"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name",
        "support.type.property-name.css",
        "support.type.vendored.property-name"
      ],
      "settings": {
        "foreground": "{blue}"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "{red}"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "{red}",
        "fontStyle": "{fontStyle}"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "foreground": "{blue}",
        "fontStyle": "{fontStyle}"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "{blue}"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "{purple}",
        "fontStyle": "{fontStyle}"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "{yellow}"
      }
    },
    {
      "name": "HTML Doctype",
      "scope": [
        "meta.tag.metadata.doctype entity.name.tag",
        "meta.tag.metadata.doctype entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "{purple}",
        "fontStyle": "{fontStyle}"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "{yellow}"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "{blue}"
      }
    },
    {
      "name": "CSS psuedo selectors",
      "scope": [
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "{teal}"
      }
    },
    {
      "name": "CSS Property value",
      "scope": ["support.constant.property-value"],
      "settings": {
        "foreground": "{pink}"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "{green}"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "{red}"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "{purple}"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "{lightCyan}"
      }
    },
    {
      "name": "Regular Expressions - Punctuation",
      "scope": ["punctuation.definition.group"],
      "settings": {
        "foreground": "{red}"
      }
    },
    {
      "name": "Regular Expressions - Character Class",
      "scope": [
        "constant.other.character-class.regexp",
        "keyword.control.anchor.regexp"
      ],
      "settings": {
        "foreground": "{purple}"
      }
    },
    {
      "name": "Regular Expressions - Character Class Set",
      "scope": ["constant.other.character-class.set.regexp"],
      "settings": {
        "foreground": "{yellow}"
      }
    },
    {
      "name": "Regular Expressions - Quantifier",
      "scope": ["keyword.operator.quantifier.regexp"],
      "settings": {
        "foreground": "{pink}"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "{cyan}"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator.js",
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "foreground": "{blue}",
        "fontStyle": "{fontStyle}"
      }
    },
    {
      "name": "CSS Units",
      "scope": ["keyword.other.unit"],
      "settings": {
        "foreground": "{darkOrange}"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "{red}"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "{blue}"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "{purple}"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "{pink}"
      }
