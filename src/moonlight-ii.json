    "inputValidation.warningBackground": "#ad7c43",
    "inputValidation.warningForeground": "#ffffff",
    "inputValidation.warningBorder": "{yellow}50",
    "dropdown.foreground": "{gray10}",
    "dropdown.background": "{gray5}",
    "dropdown.border": "#00000066",
    "list.hoverForeground": "{gray10}",
    "list.hoverBackground": "{gray3}",
    "list.activeSelectionBackground": "{gray5}",
    "list.activeSelectionForeground": "#ffffff",
